generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Query {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  fullName  String   @db.VarChar(255)
  email     String   @unique
  contact   BigInt   @unique
  query     String?
}
